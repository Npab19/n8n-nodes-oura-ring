{
  "name": "n8n-nodes-oura-ring",
  "version": "0.1.1",
  "description": "n8n nodes for Oura Ring API - access sleep, activity, and health data from your Oura Ring",
  "keywords": [
    "n8n-community-node-package",
    "oura",
    "oura ring",
    "sleep tracker",
    "health",
    "fitness",
    "wearable"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": ""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" \"credentials/**/*.ts\" --fix",
    "prepublishOnly": "npm run build && npm run lint -q"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OuraRingOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/OuraRing/OuraRing.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
